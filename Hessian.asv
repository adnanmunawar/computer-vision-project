function s_hess = Hessian(steepest)
hessX = size(steepest,2) ;
hessY = size(steepest,1) ;
s_hess =zeros(3);
hess = zeros(hessX) ;
matrixS = steepest' ;

for countx=1:3
    for county=1:3
        matrixA = steepest(:,1:countx*hessX/3) ;
        matrixB = matrixS(1:county*hessX/3,:) ;
        s_hess(countx,county) = subhessian(matrixB,matrixA) ;
    end
end
for h
figure ;
imshow(s_hess) ;
end
